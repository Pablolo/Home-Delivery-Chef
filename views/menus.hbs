<div class="menus-container">
    <h2>Menus</h2>
    <div class="menu-box">
        {{#each menus }}
        <img src="{{ this.image }}" alt="picture of menu">
        <p>{{ this.name }} {{ this.surname }}</p>
        <button class="seeMore-menu-button"><a href="/menu/{{ this._id }}">See more</a></button>
        {{/each}}
    </div>
</div>